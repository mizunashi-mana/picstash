{
  "private": true,
  "type": "module",
  "name": "@picstash/server",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint:eslint": "eslint --cache --fix src",
    "lint:deps": "depcruise src --config .dependency-cruiser.mjs",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "embedding:generate": "tsx src/cli/generate-embeddings.ts generate",
    "embedding:sync": "tsx src/cli/generate-embeddings.ts sync",
    "embedding:regenerate": "tsx src/cli/generate-embeddings.ts regenerate",
    "embedding:status": "tsx src/cli/generate-embeddings.ts status",
    "label:embedding:generate": "tsx src/cli/generate-label-embeddings.ts generate",
    "label:embedding:regenerate": "tsx src/cli/generate-label-embeddings.ts regenerate",
    "label:embedding:status": "tsx src/cli/generate-label-embeddings.ts status"
  },
  "engines": {
    "node": ">=21.0.0"
  },
  "dependencies": {
    "@picstash/core": "*",
    "@fastify/cors": "^11.2.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^9.0.0",
    "@huggingface/transformers": "^3.8.1",
    "@picstash/api": "*",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "adm-zip": "^0.5.16",
    "better-sqlite3": "^12.5.0",
    "fastify": "^5.6.2",
    "inversify": "^7.10.8",
    "js-yaml": "^4.1.1",
    "node-unrar-js": "^2.0.2",
    "pino-roll": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.5",
    "sqlite-vec": "^0.1.7-alpha.2",
    "tesseract.js": "^7.0.0",
    "unzipper": "^0.12.3",
    "yauzl-promise": "^4.0.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@picstash/eslint-config": "*",
    "@types/adm-zip": "^0.5.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/js-yaml": "^4.0.9",
    "@types/unzipper": "^0.10.11",
    "@types/yauzl-promise": "^4.0.1",
    "@vitest/coverage-v8": "^4.0.16",
    "dependency-cruiser": "^17.3.6",
    "eslint": "^9.39.2",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.2.0",
    "prisma-lint": "^0.13.0",
    "tsx": "^4.21.0",
    "vitest": "^4.0.16"
  }
}
