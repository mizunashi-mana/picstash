{
  "name": "@picstash/client",
  "version": "1.0.0",
  "description": "Picstash web client",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint:eslint": "eslint --cache --fix src",
    "lint:deps": "depcruise src --config .dependency-cruiser.mjs"
  },
  "dependencies": {
    "@mantine/core": "^8.1.4",
    "@mantine/dropzone": "^8.3.10",
    "@mantine/hooks": "^8.1.4",
    "@picstash/shared": "*",
    "@tanstack/react-query": "^5.80.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.1"
  },
  "devDependencies": {
    "@picstash/eslint-config": "*",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^4.0.16",
    "dependency-cruiser": "^17.3.6",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vitest": "^4.0.16"
  },
  "license": "Apache-2.0 OR MPL-2.0"
}
