{
  "private": true,
  "name": "@picstash/api",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "concurrently -n tsc,alias -c cyan,magenta \"tsc -w -p tsconfig.build.json\" \"tsc-alias -w -p tsconfig.build.json\"",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "lint:eslint": "eslint --cache --fix src"
  },
  "dependencies": {
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@picstash/eslint-config": "*",
    "concurrently": "^9.2.1",
    "tsc-alias": "^1.8.16"
  }
}
