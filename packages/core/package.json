{
  "private": true,
  "type": "module",
  "name": "@picstash/core",
  "version": "1.0.0",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run db:generate && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "npm run db:generate && concurrently -n tsc,alias -c cyan,magenta \"tsc -w -p tsconfig.build.json\" \"tsc-alias -w -p tsconfig.build.json\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint:eslint": "eslint --cache --fix src",
    "lint:prisma": "prisma-lint",
    "lint:deps": "depcruise src --config .dependency-cruiser.mjs",
    "db:generate": "prisma generate"
  },
  "engines": {
    "node": ">=21.0.0"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.8.1",
    "@picstash/api": "*",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "better-sqlite3": "^12.5.0",
    "inversify": "^7.10.8",
    "node-unrar-js": "^2.0.2",
    "pino-roll": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.5",
    "sqlite-vec": "^0.1.7-alpha.2",
    "tesseract.js": "^7.0.0",
    "unzipper": "^0.12.3",
    "yauzl-promise": "^4.0.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@picstash/eslint-config": "*",
    "@types/adm-zip": "^0.5.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/unzipper": "^0.10.11",
    "@types/yauzl-promise": "^4.0.1",
    "@vitest/coverage-v8": "^4.0.16",
    "adm-zip": "^0.5.16",
    "concurrently": "^9.2.1",
    "dependency-cruiser": "^17.3.6",
    "eslint": "^9.39.2",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.2.0",
    "prisma-lint": "^0.13.0",
    "tsc-alias": "^1.8.16",
    "vitest": "^4.0.16"
  }
}
